{
  "name": "@olierjs/thrift-cli",
  "description": "generate typescript code from thrift files",
  "version": "0.1.8",
  "author": "TbhT",
  "bin": {
    "thrift-cli": "bin/run.js"
  },
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/TbhT/thrift-code-gen/issues"
  },
  "dependencies": {
    "@creditkarma/thrift-parser": "^2.0.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "camelcase": "^6.3.0",
    "directory-tree": "^3.5.1",
    "schema-utils": "^4.0.0",
    "snake-case": "^3.0.4",
    "tapable": "^2.2.1",
    "ts-morph": "^23.0.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "copyfiles": "^2.4.1",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5",
    "tsc-alias": "^1.8.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/TbhT/thrift-code-gen",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "oclif": {
    "bin": "thrift-cli",
    "dirname": "thrift-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/TbhT/thrift-code-gen.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "scripts": {
    "build": "shx rm -rf dist && copyfiles -u 1 src/**/*.ejs dist && tsc -b && tsc-alias",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "prepublish": "pnpm run build"
  }
}